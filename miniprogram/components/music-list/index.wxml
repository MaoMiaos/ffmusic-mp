<view class="list">
  <view class="music-card" wx:for="{{list}}" wx:key="index">
    <view class="cover">
      <image src="../../images/yequ.jpeg"></image>
      <view class="circle"></view>
    </view>
    <view class="info">
      <view class="name">
        {{item.name}}
      </view>
      <view class="meta">
        {{'十一月的肖邦'}} {{'周杰伦'}}
      </view>
    </view>

      <image wx:if="{{likeable}}" class="likeable" src="../../icons/heart.svg"></image>
  </view>
</view>